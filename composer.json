{
    "name": "deutschepost/sdk-api-addressfactory",
    "description": "PostDirekt Addressfactory API",
    "keywords": [
        "php",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Ertner",
            "email": "sebastian.ertner@netresearch.de"
        }
    ],
    "require": {
